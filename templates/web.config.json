{
	"-path"             : "<%%{projectPwd}%%>",
	".gitignore"        : {"-file": ".gitignore"},
	"package.json"      : {"-file": "package.json"},	
	"README.md"         : {"-file": "README.md"},
	"tsconfig.json"     : {"-file": "tsconfig.json"},
	"webpack.config.js" : {"-file": "webpack.config.js"},
	"webpack": {
		"assets.config.js" : { "-file": "webpack/assets.config.js"},
		"base.config.js" : { "-file": "webpack/base.config.js"},
		"bundle.dll.config.js" : { "-file": "webpack/bundle.dll.config.js"},
		"markup.config.js" : { "-file": "webpack/markup.config.js"},
		"tyepscript.config.js" : { "-file": "webpack/typescript.config.js"}
	},
	"src": {
		"app": {
			"app-entry.ts": {"-file": "src/app/app-entry.ts"}
		},
		"templates": { 
			"-type": "d",
			"entry.ejs": { "-file": "templates/entry.ejs" },
			"browserconfig.xml": {
				"-file": "templates/browserconfig.xml"
			},
			"robots.txt": {
				"-file": "templates/robots.txt"
			}
		}		
	}
}