{
	"-path": "<%{WMCG_PWD}%>",
	"webpack.config.js": {
		"-file": "web/webpack.config.js"
	},
	"tsconfig.json": {
		"-file": "web/tsconfig.json"
	},
	"tslint.json": {
		"-file": "tslint.json"
	},
	"package.json": {
		"-file": "web/package.json"
	},
	".gitignore": {
		"-file": "web/gitignore.web"
	},
	"README.md": {
		"-file": "web/README.md"
	},
	"src": {
		"app": { 
			"-type": "d",
			"app.ts": { "-file": "web/app/app.ts" },
			"component": {
				"component.ts": { "-file": "web/app/component/component.ts" },
				"component.styl": { "-file": "web/app/component/component.styl" },
				"component.htm": { "-file": "web/app/component/component.htm" }
			}
		},
		"styles": { 
			"-type": "d",
			"_vendor_.styl": { "-file": "web/styles/_vendor_.styl" },
			"_common_.styl": { "-file": "web/styles/_common_.styl" },
			"vendor": { 
				"-type": "d",
				"reset.styl": { "-file": "web/styles/vendor/reset.styl" }
			}
		},
		"templates": { 
			"-type": "d",
			"entry.ejs": { "-file": "web/templates/entry.ejs" },
			"browserconfig.xml": {
				"-file": "web/templates/browserconfig.xml"
			},
			"robots.txt": {
				"-file": "web/templates/robots.txt"
			}
		}
	}
}